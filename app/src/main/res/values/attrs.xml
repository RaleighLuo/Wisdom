    <declare-styleable name="RoundCorners">
        <attr name="radiusX" format="dimension" />
        <attr name="radiusY" format="dimension" />
    </declare-styleable>
    <declare-styleable name="CusTextItem_Attrs">
        <attr name="cti_title" format="reference" />
